            <Color x:Key="ThemeControlHighlightHighColor">#FF808080</Color>
            <Color x:Key="HighlightColor">#FF086F9E</Color>
            <Color x:Key="HighlightColor2">#FF096085</Color>
            <Color x:Key="HyperlinkVisitedColor">#FF681DA8</Color>

            <SolidColorBrush x:Key="ThemeHoverBackgroundBrush" Color="{StaticResource ThemeHoverBackgroundColor}" />

            <SolidColorBrush x:Key="ThemeBorderLowBrush" Color="{StaticResource ThemeBorderLowColor}" />
            <SolidColorBrush x:Key="ThemeBorderMidBrush" Color="{StaticResource ThemeBorderMidColor}" />
            <SolidColorBrush x:Key="ThemeBorderHighBrush" Color="{StaticResource ThemeBorderHighColor}" />
            <SolidColorBrush x:Key="ThemeControlLowBrush" Color="{StaticResource ThemeControlLowColor}" />
            <SolidColorBrush x:Key="ThemeControlMidBrush" Color="{StaticResource ThemeControlMidColor}" />
            <SolidColorBrush x:Key="ThemeControlMidHighBrush" Color="{StaticResource ThemeControlMidHighColor}" />
            <SolidColorBrush x:Key="ThemeControlHighBrush" Color="{StaticResource ThemeControlHighColor}" />
            <SolidColorBrush x:Key="ThemeControlVeryHighBrush" Color="{StaticResource ThemeControlVeryHighColor}" />
            <SolidColorBrush x:Key="ThemeControlHighlightLowBrush"
                             Color="{StaticResource ThemeControlHighlightLowColor}" />
            <SolidColorBrush x:Key="ThemeControlHighlightMidBrush"
                             Color="{StaticResource ThemeControlHighlightMidColor}" />
            <SolidColorBrush x:Key="ThemeControlHighlightHighBrush"
                             Color="{StaticResource ThemeControlHighlightHighColor}" />
            <SolidColorBrush x:Key="HighlightBrush" Color="{StaticResource HighlightColor}" />
            <SolidColorBrush x:Key="HighlightBrush2" Color="{StaticResource HighlightColor2}" />
            <SolidColorBrush x:Key="HyperlinkVisitedBrush" Color="{StaticResource HyperlinkVisitedColor}" />

            <SolidColorBrush x:Key="RefreshVisualizerForeground" Color="Black" />
            <SolidColorBrush x:Key="RefreshVisualizerBackground" Color="Transparent" />


            <!-- BaseResources for CaptionButtons.xaml -->
            <SolidColorBrush x:Key="CaptionButtonForeground" Color="Black" />
            <SolidColorBrush x:Key="CaptionButtonBackground" Color="#ffe5e5e5" />
            <SolidColorBrush x:Key="CaptionButtonBorderBrush" Color="#ffcacaca" />

        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">

            <Color x:Key="ThemeBackgroundColor">#FF282828</Color>
            <SolidColorBrush x:Key="ThemeBackgroundBrush" Color="{DynamicResource ThemeBackgroundColor}" />
            <Color x:Key="ThemeWorkspaceColor">#ff18181A</Color>
            <SolidColorBrush x:Key="ThemeWorkspaceBrush" Color="{DynamicResource ThemeWorkspaceColor}" />

            
            <Color x:Key="ThemeWorkspace2Color">#323130</Color>
            <SolidColorBrush x:Key="ThemeWorkspace2Brush" Color="{DynamicResource ThemeWorkspace2Color}" />

            <Color x:Key="ThemeForegroundColor">#FFDEDEDE</Color>
            <SolidColorBrush x:Key="ThemeForegroundBrush" Color="{DynamicResource ThemeForegroundColor}" />

            <Color x:Key="ThemeWorkspaceMidColor">#1E1E1E</Color>
            <SolidColorBrush x:Key="ThemeWorkspaceColorMidBrush" Color="{DynamicResource ThemeWorkspaceMidColor}" />

            <Color x:Key="ThemeMenuHoverColor">#2880E6</Color>
            <SolidColorBrush x:Key="ThemeMenuHoverBrush" Color="{DynamicResource ThemeMenuHoverColor}" />


            <Color x:Key="ThemeHoverColor">#33ffffff</Color>
            <SolidColorBrush x:Key="ThemeHoverBrush" Color="{DynamicResource ThemeHoverColor}" />

            <Color x:Key="ThemeActiveColor">#66ffffff</Color>
            <SolidColorBrush x:Key="ThemeActiveBrush" Color="{DynamicResource ThemeActiveColor}" />

            <Color x:Key="ThemeIconStrokeColor">#dddddd</Color>
            <SolidColorBrush x:Key="ThemeIconStrokeBrush" Color="{DynamicResource ThemeIconStrokeColor}" />

            <Color x:Key="ThemeIconFillColor1">#444444</Color>
            <SolidColorBrush x:Key="ThemeIconFillColor1Brush" Color="{DynamicResource ThemeIconFillColor1}" />

            <Color x:Key="ThemeIconFillColor2">#cccccc</Color>
            <SolidColorBrush x:Key="ThemeIconFillColor2Brush" Color="{DynamicResource ThemeIconFillColor2}" />


            <Color x:Key="ThemeBorderColor">#595959</Color>
            <SolidColorBrush x:Key="ThemeBorderBrush" Color="{DynamicResource ThemeBorderColor}" />

            <Color x:Key="ThemeControlColor">#323130</Color>
            <SolidColorBrush x:Key="ThemeControlBrush" Color="{DynamicResource ThemeControlColor}" />

            <!-- TODO Remove unused colors -->
            
            <Color x:Key="ThemeBorderLowColor">#FF505050</Color>
            <Color x:Key="ThemeBorderMidColor">#FF808080</Color>
            <Color x:Key="ThemeBorderHighColor">#FFA0A0A0</Color>
