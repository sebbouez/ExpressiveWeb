            <Color x:Key="ThemeWorkspaceColor">#ffe8e8e8</Color>
            <SolidColorBrush x:Key="ThemeWorkspaceBrush" Color="{DynamicResource ThemeWorkspaceColor}" />

            <Color x:Key="ThemeWorkspace2Color">#D6D6D6</Color>
            <SolidColorBrush x:Key="ThemeWorkspace2Brush" Color="{DynamicResource ThemeWorkspace2Color}" />

            <Color x:Key="ThemeForegroundColor">#FF212121</Color>
            <SolidColorBrush x:Key="ThemeForegroundBrush" Color="{DynamicResource ThemeForegroundColor}" />

            <Color x:Key="ThemeWorkspaceMidColor">#FFc6c6c6</Color>
            <SolidColorBrush x:Key="ThemeWorkspaceColorMidBrush" Color="{DynamicResource ThemeWorkspaceMidColor}" />

